<Window x:Class="SeleniteSeaEditor.controls.NewBlockWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeleniteSeaEditor.controls"
        mc:Ignorable="d"
        Background="#FF0E1314"
        Title="NewBlockWindow" Height="450" Width="800">
    <Border BorderThickness="1" BorderBrush="White">
        <DockPanel>
            <TextBox DockPanel.Dock="Top" Background="Transparent"/>
            <ListBox Background="Transparent" x:Name="ActionSelectionListbox"/>
        </DockPanel>
    </Border>
</Window>

<Window x:Class="SeleniteSeaEditor.controls.Editors.EditorSSBlockScopeFunction"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeleniteSeaEditor.controls.Editors"
        mc:Ignorable="d"
        Title="EditorSSBlockScopeFunction" Height="135" Width="300">
    <Grid Margin="20">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Content="Nazwa:"/>
        <TextBox Grid.Column="1"/>
        <Label HorizontalAlignment="Center" Grid.Row="1" Grid.ColumnSpan="2" Content="PH Param Editor"/>
        <Button Click="Button_Click" Grid.Column="1" Grid.Row="2" Content="Accept"/>
    </Grid>
</Window>
